{
  "name": "react-typescript-storybook",
  "version": "4.0.1",
  "description": "Starter project for React, TypeScript and StoryBook",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": "https://github.com/aslakhellesoy/react-typescript-storybook",
  "author": "Aslak Helles√∏y",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "nyc": "nyc --reporter=html --reporter=text mocha",
    "test": "mocha",
    "compile": "tsc",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll"
  },
  "dependencies": {
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "next": "^10.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@cucumber/microdata": "^1.2.1",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addons": "6.0.28",
    "@storybook/preset-typescript": "3.0.0",
    "@storybook/react": "^6.0.28",
    "@types/jsdom": "^16.2.5",
    "@types/mocha": "8.0.3",
    "@types/node": "14.14.6",
    "@types/react-dom": "16.9.9",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "babel-loader": "^8.2.0",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "husky": "^4.3.0",
    "jsdom": "^16.4.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "react-is": "^17.0.1",
    "schema-dts": "^0.7.1",
    "source-map-support": "^0.5.19",
    "ts-loader": "8.0.9",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage",
      "stories"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  }
}
